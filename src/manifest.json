{
    "manifest_version": 3,
    "name": "holodule anywhere",
    "version": "1.2.0",

    "description": "ホロジュールをページ内に表示",

    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["content_script.js"]
    }],
    "background": {
        "service_worker": "service_worker.js"
    },
    "options_ui": {
        "page": "options_page/options.html",
        "open_in_tab": false
    },
    "permissions": [
        "storage"
    ]
}
